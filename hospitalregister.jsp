            <li class="style3"></li>
          </ul>
        </div>
        <div class="gadget">
          <h2 class="star">&nbsp;</h2>
        </div>
      </div>
      <div class="mainbar">
        <div class="article">
          <h2 align="center">Hospital Registration </h2>
          <form id="form1" name="form1" method="post" action="HospitalRegister">
            <table width="389" height="330" border="0" align="center">
              <tr>
                <td class="style11"><span class="style14">Hospital Name:</span></td>
                <td><span><span>
                  <input type="text" id="fullname" name="fullname" class="login_input" />
                </span></span></td>
              </tr>
              <tr>
                <td class="style11"><span class="style14">User Name:</span></td>
                <td><span><span>
                  <input name="username" type="text" class="login_input" id="username" />
                </span></span></td>
              </tr>
              <tr>
                <td class="style11"><span class="style14">Password:</span></td>
                <td><input type="password" id="password" name="password" class="login_input" /></td>
              </tr>
              <tr>
                <td class="style11"><span class="style14">Address:</span></td>
                <td><span><span>
                  <input type="text" id="address" name="address" class="login_input" />
                </span></span></td>
              </tr>
              <tr>
                <td class="style11"><span class="style14">Mobile:</span></td>
                <td><span><span>
                  <input type="text" id="mobile" name="mobile" class="login_input" />
                </span></span></td>
              </tr>
              <tr>
                <td class="style11"><span class="style14">Email Id:</span></td>
                <td><span><span>
                  <input type="text" id="emailid" name="emailid" class="login_input" />
                </span></span></td>
              </tr>
              <tr>
                <td colspan="2"><div align="center">
                    <label>
                    <input type="submit" name="Submit" value="Submit" />
                    </label>
                </div></td>
              </tr>
              <tr>
                <td colspan="2"><div align="center" class="style7"><a href="hospitallogin.jsp" class="style12">Hospital Login </a></div></td>
              </tr>
            </table>
          </form>
          <p>&nbsp;</p>
        </div>
        <div class="article"></div>
      </div>
      <p>&nbsp;</p>
